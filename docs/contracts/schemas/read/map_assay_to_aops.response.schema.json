{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "map_assay_to_aops.response",
  "type": "object",
  "required": ["results"],
  "properties": {
    "results": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["aop", "assay_id"],
        "properties": {
          "assay_id": {"type": "string"},
          "aop": {
            "type": "object",
            "required": ["id", "iri", "title"],
            "properties": {
              "id": {"type": "string"},
              "iri": {"type": "string", "format": "uri"},
              "title": {"type": ["string", "null"]}
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false
}
